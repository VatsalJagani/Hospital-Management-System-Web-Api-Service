<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>Root</param-name>
        <param-value>/HmsClient</param-value>
    </context-param>
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>servlets.CheckPatientID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCheckins</servlet-name>
        <servlet-class>servlets.GetCheckins</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/CheckPatientID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCheckins</servlet-name>
        <url-pattern>/GetCheckins</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
